{
  "name": "donglin-temple-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc -w -p tsconfig.json & nodemon dist/src/index.js",
    "start:build": "tsc -w -p tsconfig.json",
    "start:dev": "tsc -w -p tsconfig.json & NODE_ENV=dev nodemon dist/src/index.js",
    "start:prod": "tsc -w -p tsconfig.json & NODE_ENV=prod nodemon dist/src/index.js",
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint . --ext .ts --ext .mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "http-status-codes": "^2.2.0",
    "morgan": "~1.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
