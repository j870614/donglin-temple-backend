{
  "name": "donglin-temple-backend",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "concurrently \"tsc -w -p tsconfig.json\" \"nodemon dist/src/index.js\"",
    "start:build": "tsc -w -p tsconfig.json",
    "start:dev": "concurrently \"tsoa spec-and-routes\" \"onchange -a 'src/controllers/*.ts' -- tsoa spec-and-routes\" \"tsc -w -p tsconfig.json\" \"cross-env NODE_ENV=dev nodemon dist/src/index.js\"",
    "start:prod:build": "pnpm i && pnpm prisma db pull && pnpm prisma generate && pnpm tsoa spec-and-routes && pnpm tsc -p tsconfig.json",
    "start:prod": "nodemon dist/src/index.js",
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint . --ext .ts --ext .mts --fix --ignore-path .gitignore",
    "prisma:gen": "run-s prisma:db prisma:generate",
    "prisma:db": "prisma db pull",
    "prisma:generate": "prisma generate",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@tsoa/runtime": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.1.4",
    "express": "~4.18.2",
    "http-status-codes": "^2.2.0",
    "jest-mock-extended": "^3.0.4",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "morgan": "~1.10.0",
    "mysql": "^2.18.1",
    "onchange": "^7.1.0",
    "passport": "^0.6.0",
    "rxjs": "^7.8.1",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.6.3",
    "tsoa": "^5.1.1",
    "uuid": "^9.0.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.2.5",
    "@types/passport": "^1.0.12",
    "@types/promise.any": "^2.0.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.17",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "concurrently": "^8.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-extended": "^4.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
