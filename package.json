{
  "name": "donglin-temple-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"tsc -w -p tsconfig.json\" \"nodemon dist/src/index.js\"",
    "start:build": "tsc -w -p tsconfig.json",
    "start:dev": "concurrently \"tsc -w -p tsconfig.json\" \"cross-env NODE_ENV=dev nodemon dist/src/index.js\"",
    "start:prod": "concurrently \"tsc -w -p tsconfig.json\" \"cross-env NODE_ENV=prod nodemon dist/src/index.js\"",
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint . --ext .ts --ext .mts --fix --ignore-path .gitignore",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/validator": "^13.7.15",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "dotenv": "^16.0.3",
    "express": "~4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "~1.10.0",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "jest-extended": "^3.2.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
